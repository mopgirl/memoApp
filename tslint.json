{
  "extends": [
    "tslint-config-airbnb"
  ],
  "rules": {
    "ter-indent": [
      true,
      2
    ],
    "max-line-length": [
      true,
      120
    ],
    "object-shorthand-properties-first": false,
    "radix": false,
    "ter-arrow-parens": false,
    "no-sparse-arrays": true,
    "trailing-comma": [
      true,
      {
        "multiline": "always"
      }
    ]
  }
}
